<script> 
$(document).ready(function(){
    $("#block_button").click(function(){
        var div = $("div");
        div.animate({height: '300px', opacity: '0.4'}, "slow");
        div.animate({width: '300px', opacity: '0.8'}, "slow");
        div.animate({height: '100px', opacity: '0.4'}, "slow");
        div.animate({width: '100px', opacity: '0.8'}, "slow");
    });
});
</script>

	<button id = "block_button">Block Play</button>

	<div id = "green_block" style="background:#98bf21;height:100px;width:100px;"></div>

	<div id = "blue_block" style="background:blue;height:100px;width:100px;"></div>
